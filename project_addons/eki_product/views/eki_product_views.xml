<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="eki_base_product_template_form_view" model="ir.ui.view">
            <field name="name">eki.base.product.template.form.view</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="eki_base.eki_base_product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='eki_ldap']" position="after">
                    <field name="eki_origin_country_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>